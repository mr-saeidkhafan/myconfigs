(custom-set-faces
 ;; custom-set-faces was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(default ((t (:inherit nil :extend nil :stipple nil :background "black" :foreground "white" :inverse-video nil :box nil :strike-through nil :overline nil :underline nil :slant normal :weight normal :height 113 :width normal :foundry "SRC" :family "Hack"))))
 '(comint-highlight-input ((t (:weight bold))))
 '(cursor ((t (:background "white"))))
 '(doom-visual-bell ((t (:background "red"))))
 '(font-lock-comment-delimiter-face ((t (:inherit font-lock-comment-face :slant italic))))
 '(font-lock-comment-face ((t (:foreground "#666585" :slant italic))))
 '(mode-line ((t (:background "#151515" :foreground "white" :box nil))))
 '(mouse-drag-and-drop-region ((t (:inherit default)))))

;; Set up package.el to work with MELPA
(require 'package)
(add-to-list 'package-archives '("melpa" . "https://melpa.org/packages/"))
(package-initialize)

;; Enable Evil
(use-package evil
  :init
  (setq evil-undo-system 'undo-fu))
(evil-mode 1)

;; Enable line number globaly
(global-linum-mode t)
(setq inhibit-startup-message t)

;; undo fu
(use-package undo-fu
  :config
  (define-key evil-normal-state-map "u" 'undo-fu-only-undo)
  (define-key evil-normal-state-map "\C-r" 'undo-fu-only-redo))

(use-package undo-fu-session
  :config
  (setq undo-fu-session-incompatible-files '("/COMMIT_EDITMSG\\'" "/git-rebase-todo\\'")))

(global-undo-fu-session-mode)

;; Elpy (python ide)
(use-package elpy
  :ensure t
  :init
  (elpy-enable))

;; theme
(use-package doom-themes
  :config
  ;; Global settings (defaults)
  (setq doom-themes-enable-bold t    ; if nil, bold is universally disabled
        doom-themes-enable-italic t) ; if nil, italics is universally disabled
  (load-theme 'doom-one t)

  ;; Enable flashing mode-line on errors
  (doom-themes-visual-bell-config)

  ;; Corrects (and improves) org-mode's native fontification.
  (doom-themes-org-config))

;; disable creating backup file
(setq make-backup-files nil)
(setq create-lockfiles nil)
(setq auto-save-default nil)
